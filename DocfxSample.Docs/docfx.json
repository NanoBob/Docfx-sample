{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**/DocfxSample.ClassLib.csproj"
          ]
        }
      ],
      "dest": "api/lib",
      "namespaceLayout": "nested"
    },
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**/DocfxSample.Events*.csproj"
          ]
        }
      ],
      "dest": "api/events",
      "namespaceLayout": "nested"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      },
      {
        "src": "../DocfxSample.Api/bin/Debug/net8.0",
        "files": [
          "swagger.json"
        ],
        "dest": "api/api"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "DocfxSample",
      "_appTitle": "DocfxSample",
      "_enableSearch": true,
      "pdf": false
    }
  }
}